# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Sanity CMS Configuration
VITE_SANITY_PROJECT_ID=your_sanity_project_id
VITE_SANITY_DATASET=your_sanity_dataset
VITE_SANITY_API_TOKEN=your_sanity_api_token

# Security Configuration
VITE_SESSION_SECRET=your_session_secret_key
VITE_CSRF_SECRET=your_csrf_secret_key

# Email Configuration (for notifications)
VITE_SMTP_HOST=smtp.gmail.com
VITE_SMTP_PORT=587
VITE_SMTP_USER=your_email@gmail.com
VITE_SMTP_PASS=your_app_password

# Virus Scanning API (optional)
VITE_VIRUSTOTAL_API_KEY=your_virustotal_api_key

# Webhook URLs (optional)
VITE_SECURITY_WEBHOOK_URL=https://your-webhook-endpoint.com/security

# Environment
NODE_ENV=development